<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>

    <record id="export_feature_form" model="ir.ui.view">
        <field name="name">wiz.prestashop.export.feature.form</field>
        <field name="model">wiz.prestashop.export.feature</field>
        <field name="arch" type="xml">
            <form string="Export Product Features">
                <group>
                    <field name="backend_id"/>
                </group>
                <footer>
                    <button name="export_features" type="object"
                            string="Export" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link"
                            special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window id="act_export_prestashop_features"
                name="Export To PrestaShop"
                res_model="wiz.prestashop.export.feature"
                src_model="custom.info.property"
                view_mode="form"
                target="new"
                key2="client_action_multi"
                view_id="export_feature_form"/>

</data>
</openerp>
